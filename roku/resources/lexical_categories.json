{
    "categories": [{
        "token": "identifier",
        "regex": "(_|[A-Za-z])(_|[A-Za-z]|[0-9])*",
        "action": "check_value",
        "is_unique": false
    }, {
        "token": "line_end",
        "regex": ";",
        "is_unique": true
    }, {
        "token": "op_arit",
        "regex": "\\+|\\-|\\*|\\/|%",
        "is_unique": false
    }, {
        "token": "op_not",
        "regex": "!",
        "is_unique": true
    }, {
        "token": "op_log",
        "regex": "\\|\\||&&",
        "is_unique": false
    }, {
        "token": "op_comp",
        "regex": "==|!=|<=|>=|>|<",
        "is_unique": false
    }, {
        "token": "asig",
        "regex": "=",
        "is_unique": true
    }, {
        "token": "zhuli",
        "regex": "-?[0-9]+",
        "is_unique": false
    }, {
        "token": "appa",
        "regex": "-?[0-9]+\\.[0-9]+",
        "is_unique": false
    }, {
        "token": "arg_start",
        "regex": "\\(",
        "is_unique": true
    }, {
        "token": "arg_end",
        "regex": "\\)",
        "is_unique": true
    }, {
        "token": "block_start",
        "regex": "{",
        "is_unique": true
    }, {
        "token": "block_end",
        "regex": "}",
        "is_unique": true
    }, {
        "token": "scroll",
        "regex": "\".*\"",
        "is_unique": false
    }, {
        "token": "wan",
        "regex": "'.'",
        "is_unique": false
    }, {
        "token": "incomplete_appa",
        "regex": "-?[0-9]+\\.",
        "is_unique": false
    }, {
        "token": "incomplete_scroll_wan",
        "regex": "\"[^\"]*|'.",
        "is_unique": false
    }, {
        "token": "scroll_wan_delimiter",
        "regex": "\"|'",
        "is_unique": false
    }, {
        "token": "incomplete_op_log",
        "regex": "\\||&",
        "is_unique": false
    }, {
        "token": "colon",
        "regex": ":",
        "is_unique": true
    }, {
        "token": "comma",
        "regex": ",",
        "is_unique": true
    }],

    "reserved": [{
        "token": "agni",
        "regex": "agni",
        "is_unique": true
    },{
        "token": "agnikai",
        "regex": "agnikai",
        "is_unique": true
    }, {
        "token": "kai",
        "regex": "kai",
        "is_unique": true
    }, {
        "token": "avatar",
        "regex": "avatar",
        "is_unique": true
    }, {
        "token": "raava",
        "regex": "raava",
        "is_unique": true
    }, {
        "token": "cabbage",
        "regex": "cabbage",
        "is_unique": true
    }, {
        "token": "yipyip",
        "regex": "yipyip",
        "is_unique": true
    }, {
        "token": "energy",
        "regex": "energy",
        "is_unique": true
    }, {
        "token": "bend",
        "regex": "bend",
        "is_unique": true
    }, {
        "token": "type",
        "regex": "zhuli|appa|koi|scroll|wan",
        "is_unique": false
    }, {
        "token": "koi",
        "regex": "tui|la",
        "is_unique": false
    }, {
        "token": "null",
        "regex": "sokka",
        "is_unique": true
    }],

    "spaces": {
        "regex": "\\s"
    }
}
